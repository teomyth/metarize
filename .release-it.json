{
  "git": {
    "commitMessage": "chore: release v${version}",
    "tagName": "v${version}",
    "requireCleanWorkingDir": true
  },
  "github": {
    "release": true,
    "releaseName": "Release v${version}",
    "web": true
  },
  "npm": {
    "publish": false
  },
  "hooks": {
    "after:bump": "pnpm build",
    "after:release": "echo Successfully released ${name} v${version} to GitHub."
  },
  "plugins": {}
}
